prototype = ( ~comment )* ;

comment{comment.line.number-sign}
= '#+'{punctuation.definition.comment}
  ~'$\n?'
;

main
= ( ~( test-simple-passive
     # | test-push-passive
     )
  )*
;

test-simple-passive{simple-passive}
= '\btest-simple-passive\b'
  `"`{a}
  ( ~`foo`{b} )*
  ~`"`{c}
;

